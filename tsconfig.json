{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "noEmit": true,
    "paths": {
      "#/*": ["./src/*"]
    }
  },
  "include": [
    "src/**/*",
    "src/**/*.vue",
    "test/**/*"
  ]
}
