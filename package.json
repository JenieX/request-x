{
  "name": "request-x",
  "version": "3.0.3",
  "private": true,
  "description": "Request X",
  "author": "Gerald <gera2ld@live.com>",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint:ts": "vue-tsc",
    "lint:js": "eslint src",
    "lint": "pnpm /^lint:/",
    "format": "prettier -w src",
    "dev:background": "NODE_ENV=development vite build --watch",
    "dev:content": "NODE_ENV=development ENTRY=content vite build --watch",
    "dev": "gulp copyAssets && pnpm run /^dev:/",
    "build:background": "vite build",
    "build:content": "ENTRY=content vite build",
    "build:connector": "ENTRY=connector vite build && tsc src/connector/index.ts -d --emitDeclarationOnly --declarationDir lib --skipLibCheck",
    "build": "pnpm lint && pnpm test && gulp clean && pnpm /^build:/ && gulp copyAssets",
    "test": "vitest --run"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gera2ld/request-x.git"
  },
  "bugs": {
    "url": "https://github.com/gera2ld/request-x/issues"
  },
  "homepage": "https://github.com/gera2ld/request-x#readme",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@iconify/json": "^2.2.322",
    "@types/webextension-polyfill": "^0.12.3",
    "@unocss/postcss": "66.1.0-beta.8",
    "@vitejs/plugin-vue": "^5.2.3",
    "del": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "gulp": "^5.0.0",
    "husky": "^9.1.7",
    "jimp": "^1.6.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.5.0",
    "postcss-nesting": "^13.0.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "unocss": "66.1.0-beta.8",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.2.4",
    "vitest": "^3.1.1",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.36.5",
    "@unocss/reset": "66.1.0-beta.8",
    "@violentmonkey/shortcut": "^1.4.4",
    "codemirror": "^6.0.1",
    "es-toolkit": "^1.34.1",
    "vue": "^3.5.13",
    "webextension-polyfill": "^0.12.0"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ]
  }
}
