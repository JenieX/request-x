  },
  "homepage": "https://github.com/gera2ld/request-blocker#readme",
  "devDependencies": {
    "@gera2ld/plaid": "^1.0.0",
    "@gera2ld/plaid-vue": "^1.0.0",
    "gulp": "^4.0.0",
    "husky": "^1.3.1",
    "plugin-error": "^1.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "core-js": "^2.6.2",
    "vue": "^2.5.22",
    "vueleton": "^0.6.1"
  }
